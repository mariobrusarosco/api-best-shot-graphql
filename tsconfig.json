{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "lib": ["es6"],
    "outDir": "./dist",
    "rootDir": "./",
    "baseUrl": "./",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "types": ["node"],
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
